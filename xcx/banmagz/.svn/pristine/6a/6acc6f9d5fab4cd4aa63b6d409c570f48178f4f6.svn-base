/* message/bmDialog.wxss */
page{background: #f5f5f5}
.content{width: 100%;height: 100%;padding-bottom: 50rpx;}
.list{width: 100%}
.msgempty{width: 100%;height: 50rpx;font-size: 50rpx;line-height: 50rpx;color: #ccc;font-weight: bold;text-align: center;position: fixed;left: 0;top: 40%}
.msg_bk{width: 630rpx;margin: 20rpx auto 10rpx;background: #fff;border-radius: 10rpx;padding: 30rpx;}
.title{font-size: 36rpx;line-height: 50rpx;}
.msg_img{width: 100%;height: 350rpx;border-radius: 10rpx;overflow: hidden;margin-top: 10rpx;}
.msg_img image{width: 100%;height: 100%}
.msg_desc{font-size: 30rpx;color: #666666;line-height: 50rpx;margin-top: 10rpx;}
.msg_link{font-size: 24rpx;color: #999;line-height: 50rpx;margin-top: 10rpx;}
.time_content{width: 100%;text-align: center;margin-top: 20rpx;}
.time{display: inline-block;font-size: 20rpx;padding: 10rpx;background: rgba(0, 0, 0, 0.1);color: #fff;margin: 0 auto;border-radius: 10rpx}