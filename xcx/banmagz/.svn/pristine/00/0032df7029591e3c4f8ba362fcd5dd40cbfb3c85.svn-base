page{background: #fafafa}
.tapbar{ display: flex; align-items: center; justify-content: space-between; height: 90rpx; padding: 0 35rpx; background: #fede01;}
.pickerItem{ display: flex; align-items: center; font-size: 38rpx; color: #000000;}
.pickerItem image{ display: block; width: 28rpx; height: 16rpx; margin-left: 15rpx;}
.screenCantainer{ display: flex; align-items: center; width: 100%; height: 88rpx;justify-content: space-around;border-bottom: 1rpx solid #ccc;background: #fff}
.select{display: flex;align-items: center;justify-content: center;}
.select text{font-size: 30rpx;margin: 0 20rpx;}
.fgx{width: 2rpx;height: 32rpx;background: #cccccc}
.select_icon{width: 30rpx;height: 20rpx;}
.list{width: 100%;height: 940rpx;background: #fff}
.list_main{width: 690rpx;margin-left:30rpx;padding-right:30rpx;border-bottom: 1rpx solid #eeeeee;padding-top: 33rpx;padding-bottom: 46rpx;}
.list_top{width: 100%;height: 140rpx;display: flex;justify-content: space-between}
.list_top_left{display: flex;align-items: center;}
.avatar{width: 138rpx;height: 138rpx;border: 1rpx solid #dfdfdf;border-radius: 3rpx;display: flex;align-items: center}
.avatar image{width: 100%}
.info{margin-left: 22rpx;}
.name{font-size: 34rpx;line-height: 48rpx;}
.position{font-size: 26rpx;color: #a9a9a9;line-height: 36rpx;margin-top: 10rpx;}
.edu{font-size: 26rpx;color: #a9a9a9;line-height: 36rpx;margin-top: 7rpx;}
.price{font-size: 36rpx;color: #fe5a01;font-weight: bold;line-height: 44rpx;}
.price_yq{font-size: 24rpx;color: #bfbfbf;line-height: 36rpx;margin-top: 14rpx;text-align: center}
.content{font-size: 28rpx;color: #666666;line-height: 34rpx;margin-top: 30rpx; }
.list_bottom{display: flex;align-items: center;height: 30rpx;margin-top: 40rpx;}
.list_bottom image{width: 30rpx;height: 30rpx;}
.list_bottom text{font-size: 26rpx;color: #808191;margin: 0 30rpx 0 14rpx;}
.tabbar{width: 100%;height: 87rpx;border-top: 1rpx solid #cccccc;background: #f7f7fa;position: fixed;left: 0;bottom: 0;display: flex;justify-content: space-around;}
.tabbar_content{height: 87rpx;font-size: 0}
.tabbar_icon{width: 46rpx;height: 46rpx;margin-top: 10rpx;}
.tabbar_icon02{width: 46rpx;height: 46rpx;}
.tabbar_content view{display: block;font-size: 20rpx;color: #7f8389;line-height: 24rpx;margin-top: 0rpx;text-align: center}

