.rzCantainer{ position: absolute; left: 50%; top: 50%; margin-left: -292.5rpx; margin-top: -400rpx; width: 585rpx; height: 800rpx; z-index: 12}
.rzCantainer cover-image{ width: 585rpx; height: 800rpx;}

.lingCantainer{ position: absolute; top: 20rpx; left: 50%; margin-left: -352rpx; display: flex; align-items: center; width: 714rpx; background: #fff; border-radius: 4rpx; box-shadow: 0px 2px 15px 0px rgba(0, 0, 0, 0.1);padding: 30rpx; box-sizing: border-box}
.ling{ width: 23rpx; height: 26rpx;}
.lingTxt{ width: 600rpx; font-size: 24rpx; color: #333; margin-left: 15rpx;}

.boxCantainer{ position: absolute; top: 0; left: 0; width: 100%; height: 100%}
.boxItem{ position: absolute; left: 50%; top: 50%; margin-left: -275rpx; margin-top: -275rpx; width: 550rpx; height: 550rpx;}
.sing-com{
  position: absolute; 
  border-radius: 50%; 
  top: 50%; 
  left: 50%; 
  opacity: 1;
}

.one { 
  width: 0; 
  height: 0; 
  box-shadow: inset 0px 0px 18px 0px rgba(59, 160, 230, 0.8);
  border: 1rpx slid #3ba0e6;
  background-color: rgba(59, 160, 230,0.2);
  z-index: 2;
  margin-left: 0;
  margin-top: 0;
  animation: halo 3s 0s ease-out;
}
.two { 
  width: 0; 
  height: 0; 
  box-shadow: inset 0px 0px 18px 0px rgba(59, 160, 230, 0.8);
  border: 1rpx slid #3ba0e6;
  background-color: rgba(59, 160, 230,0.2);
  z-index: 2;
  margin-left: 0;
  margin-top: 0;
  animation: halo 3s 0.5s ease-out;
}
.three { 
  width: 0; 
  height: 0; 
  box-shadow: inset 0px 0px 18px 0px rgba(59, 160, 230, 0.8);
  border: 1rpx slid #3ba0e6;
  background-color: rgba(59, 160, 230,0.2);
  z-index: 2;
  margin-left: 0;
  margin-top: 0;
  animation: halo 3s 1s ease-out;
}
.four { 
  width: 0; 
  height: 0; 
  box-shadow: inset 0px 0px 50rpx 0px rgba(59, 160, 230, 0.8);
  border: 1rpx slid #3ba0e6;
  z-index: 2;
  margin-left: 0;
  margin-top: 0;
  animation: halo 3s 1.5s ease-out;
}

@keyframes halo {
  0% { 
    opacity: 0; 
    width: 0;
    height: 0;
    margin-left: 0;
    margin-top: 0
  }
  60%{
    opacity: 1;
    width: 550rpx;
    height: 550rpx;
    margin-left: -275rpx;
    margin-top: -275rpx
  } 
  100%{
    opacity: 0;
    width: 550rpx;
    height: 550rpx;
    margin-left: -275rpx;
    margin-top: -275rpx
  } 
}
