/* business/demand/demand01.wxss */
page{background: #f5f5f5}
.top{width: 660rpx;height: 178rpx;background: #fff;box-shadow: 0px 1px 5px 0px rgba(199, 199, 199, 0.004);padding: 72rpx 45rpx 0;}
.top_main_01{height: 54rpx;line-height: 54rpx;display: flex;justify-content: space-between}
.top_text_01{font-size: 36rpx;color: #000;font-weight: bold}
.top_text_02{font-size: 30rpx;color: #0060ff;}
.top_main_02{height: 36rpx;line-height: 36rpx;font-size: 24rpx;color: #666666;margin-top: 12rpx;}

.content_title{width: 660rpx;height: 64rpx;line-height: 64rpx;font-size: 24rpx;color: #666666;margin-top: 33rpx;padding: 0 45rpx;}
.content{width: 690rpx;padding: 0 30rpx;background: #fff}
.content_list{width: 100%;height: 87rpx;border-bottom:1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center}
.list_left{display: flex;align-items: center}
.list_icon{display: flex;align-items: center;width: 44rpx;height: 44rpx;border: 1rpx solid #c9c9c9;border-radius: 50%;position: relative}
.list_icon icon{position: absolute;top: -1rpx;left: -1rpx}
.list_text{font-size: 34rpx;color: #000;margin-left: 23rpx;}

.btnBlack{width:690rpx;height: 96rpx;position: fixed;left: 50%;margin-left: -345rpx;bottom: 50rpx;}