/* pages/business/my/companyinfo.wxss */
page{background: #f4f4f4;font-size: 30rpx;}
.main{width: 690rpx;padding:0 30rpx;background: #fff;padding-top:20rpx;padding-bottom:30rpx;}
.main-title{line-height: 50rpx;font-size: 24rpx; padding-left: 30rpx;}
.input{width: 690rpx;padding: 30rpx 0;}
.input_02{width: 690rpx;height: 65rpx;display: flex;justify-content: space-around;align-items: center;background:#f4f4f4;}
.input_title{text-align: left;}
.input_content{width: 100%;font-size: 24rpx;color: #a9a9a9;}
.input_content input{width: 630rpx;height: 70rpx;background: #f4f4f4;margin-top: 30rpx;padding: 0 30rpx;border-radius: 5rpx}
.btn button{width: 580rpx;height: 76rpx;border-radius: 38rpx;margin: 37rpx auto 0;background: linear-gradient(to right, #fede01 , #fdc100);line-height: 76rpx;font-size: 30rpx;}
.tishi{position:relative;left:600rpx;bottom:50rpx;z-index: 1}.display{display: none}
.input_bd{width: 500rpx;height: 88rpx;display: flex;align-items: center;}
.input_bd picker{width: 500rpx;height: 88rpx;overflow: hidden;padding-right: 20rpx;}
.input_bg image{width: 16rpx;height: 25rpx;}
.picker{width: 100%;height: 88rpx;font-size: 30rpx;color: #bdbdbd;line-height: 88rpx;text-align: right;}

.t_title{line-height: 100rpx;padding: 0 30rpx}
.main_textarea textarea{width: 630rpx;height: 240rpx;margin: 0 auto;background: #fff;border-radius: 10rpx;padding: 30rpx;color: #ccc}

.region_picker{width: 690rpx;height: 88rpx;font-size: 30rpx;color: #bdbdbd;text-align: right;display: flex;justify-content: space-between;align-items: center;margin: 0 auto ;}
.region_style{width: 30%;height: 50rpx;background: #fff;line-height: 50rpx;text-align: center;overflow: hidden}

.complishinfoContainer{position: absolute;left: 0; top: 0; bottom:0; right:0;width: 100%;height: 100%;}
.complishinfoMain{width: 667rpx;margin: 0 auto}
.complishinfoMainTop{padding:30rpx 0 0 0}
.complishinfoTitle{font-size: 56rpx;color: #010101;text-align: center;font-weight:bold;line-height: 129rpx}
.complishinfolook{font-size: 28rpx;color: #8e9091;text-align: center;padding-bottom: 60rpx}
.section{ position: relative; width: 100%;font-size: 30rpx;color: #8e9091;background: #fff}
.section .username{width: 100%;height: 80rpx; padding-left:35rpx;}
.section .address{width: 100%;height: 80rpx; padding-left:35rpx;}
.section .company{width: 690rpx;height: 80rpx;padding: 0 30rpx;}
.complishMap{width: 100%;padding-top:34rpx;display: flex;align-items: center;justify-content: center}
.showlogo{padding-left:76rpx;} 
.showposition{font-size: 30rpx;color: #000000;line-height: 50rpx;margin-left: 12rpx;}
.mapbtn{width: 160rpx;height: 50rpx;line-height: 50rpx;font-size: 26rpx;margin:0 auto;}
.showmap{width: 100%;height: 336rpx;padding-top: 10rpx}
.comfirmlog{height: 96rpx;width: 670rpx;background: #000000;color: white;font-size: 29rpx;border-radius: 5rpx;line-height: 96rpx;position: absolute;bottom: 34rpx;z-index: 1;left: 50%;margin-left: -335rpx;}
.show{display: block}
.hide{display: none}
.searchList{ position: absolute; width: 100%; padding: 20rpx 30rpx; box-sizing: border-box; box-shadow: 0 5rpx 18rpx #eee; border-radius: 5rpx; z-index: 11; background: #fff; border:1rpx solid #eee;}
.searchList view{ display: flex; align-items: center; justify-content: space-between; padding: 8rpx 0;}
.adresTitle{ font-size: 28rpx; color: #666;}
.cityTxt{ font-size: 22rpx; color: #aaa;}