/* pages/business/position/relProject.wxss */
page{background: #f4f4f4}
.go_pc{width: 100%;height: 88rpx;background: #f6f0c3;font-size: 28rpx;}
.go_pc_content{width: 690rpx;height: 88rpx;margin: 0 auto;line-height: 88rpx;display: flex;justify-content: space-between;align-items: center;}
.go_pc_content image{width: 16rpx;height: 25rpx;}
.b_main{width: 716rpx;background: #fff;padding-left: 34rpx;}
.b_content{width: 716rpx;background: #fff;padding-left: 34rpx;}
.b_input{width: 686rpx;height:87rpx;border-bottom: 1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center;padding-right: 30rpx;}
.b_input_02{width: 686rpx;height:87rpx;display: flex;justify-content: space-between;align-items: center;padding-right: 30rpx;}
.b_input_avatar{width: 686rpx;height:200rpx;border-bottom: 1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center;padding-right: 30rpx;}
.b_input_right{display: flex;align-items: center}
.b_input_title_01{font-size: 30rpx;color: #000}
.b_input_title_02{font-size: 24rpx;color: #888888}
.b_input_icon{width: 16rpx;height: 25rpx;margin-left: 30rpx;}
.b_input_right_avatar{width: 120rpx;height: 120rpx;border-radius: 50%;background: #000;margin-right: 35rpx;overflow: hidden;}
.b_input_right_avatar image{width: 120rpx;height: 120rpx;}
.noticeTip{ padding:10rpx 34rpx; color: #888;  font-size: 30rpx;}
.btn{width: 100%;position: fixed;left: 0;bottom: 0;}
.btn text{display: block;width: 100%;line-height: 66rpx;font-size: 20rpx;color: #999999;text-align: center;}
.btn button{width: 690rpx;height: 96rpx;background: #000;font-size: 29rpx;color: #fff;line-height: 96rpx;margin-bottom: 30rpx;}
.b_input_title_03{
  width: 400rpx;
  overflow:hidden;
  text-overflow:ellipsis; 
  white-space: nowrap;
}