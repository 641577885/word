.positiontype2Container{width: 100%;height: 100%;}
.positiontype2Main{width: 667rpx;margin: 0 auto}
.positiontype2MainTop{ position: relative; padding:49rpx 0 0 0}
.cancelBtn{ position: absolute; right:0; top: 50rpx;font-size: 30rpx; color: #0060ff}
.positiontype2MainTopC{display: flex}
.positiontype2Title{width: 602rpx;font-size:36rpx;color: #000000;font-weight:bold;line-height: 95rpx}
.positiontype2MainTopC .cancel{line-height: 95rpx;font-size: 30rpx;color: #0060ff;float: right;}
.positiontype2look{font-size: 24rpx;color: #666666;padding-bottom: 100rpx}
.positiontype2Map{width: 100%;height: 438rpx}
.positionTime{width: 100%}
.positionwages{width: 100%}
.positiononline,.wagesInput{ display: flex; justify-content: flex-start; align-items: center;height: 90rpx;}
.online{width: 140rpx;height: 58rpx;background: #fff;line-height: 58rpx;font-size: 26rpx;color: #172434;margin: 0rpx 30rpx 30rpx 0;}
.online.active{background: #fede01;}
.wagesInput{ justify-content: space-between}
.wagesInput view{ width: 268rpx; color: #000; background: #f4f4f4; border-radius: 10rpx; border: 1rpx solid #dae2f8; font-size: 30rpx; height: 80rpx; line-height: 80rpx; text-align: center; }
.wagesInput text{ color: #666; font-size: 30rpx; margin: 0 10rpx; line-height: 60rpx;font-weight: bold}


.section{width: 100%;border-bottom: 1rpx solid #e9eefb;font-size: 30rpx;color: #8e9091;display: flex}
.section text{line-height: 109rpx}
.section .wages{width: 100%;height: 109rpx;padding-left: 33rpx;color: #8e9091;font-size: 30rpx}
.positiontype2MainBottom{position: fixed;bottom: 34rpx;} 
.comfirmnext{height: 96rpx;width: 667rpx;background: #000000;color: white;font-size: 29rpx;border-radius: 5rpx;line-height: 96rpx;} 
.showtime{display: none}
.showwages{display: block}  

.positiononline02{width: 660rpx;height:92rpx;display: flex;background: #000;border-radius: 5rpx;margin-bottom: 70rpx;justify-content: center;align-items: center}
.online02{width: 310rpx;height: 76rpx;line-height: 76rpx;background: #000;font-size: 30rpx;}
.online02.active{background: #fede01;color: #000}
.online02.noactive{color: #fff}

.CoverView{ position: absolute; top: 0;left: 0;right: 0; bottom: 0; background: rgba(0,0,0,0.6);}
.CoverViewItem{ position: absolute; bottom: 0; width: 100%; left: 0; right: 0; background: #fff}
.cancelView{ height: 80rpx; display: flex; align-items: center; justify-content: space-between; padding: 0 50rpx; border-bottom: 1rpx solid #eee}
picker-view-column view{ text-align: center; height: 40px !important}
.cancel,.click{ font-size: 36rpx;}
.click{ color: #1aad19;}
.priceTxt{color: #8e9091; font-size: 26rpx;}