/* business/demand/demand02.wxss */
page{background: #f5f5f5}
.top{width: 660rpx;height: 178rpx;background: #fff;box-shadow: 0px 1px 5px 0px rgba(199, 199, 199, 0.004);padding: 72rpx 45rpx 0;}
.top_main_01{height: 54rpx;line-height: 54rpx;display: flex;justify-content: space-between}
.top_text_01{font-size: 36rpx;color: #000;font-weight: bold}
.top_text_02{font-size: 30rpx;color: #0060ff;}
.top_main_02{height: 36rpx;line-height: 36rpx;font-size: 24rpx;color: #666666;margin-top: 12rpx;}

.content{width: 690rpx;background: #fff;margin-top: 30rpx;padding: 0 30rpx 30rpx;}
.content_01{width: 100%;height: 87rpx;border-bottom:1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center;}
.content_title{font-size: 34rpx;color: #000;}
.content_gg{font-size: 34rpx;color: #0060ff}
.content_02{width: 100%;height: 87rpx;border-bottom:1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center;margin-top: 30rpx;}
.content_02_title{font-size: 30rpx;color: #666;display: block;height: 87rpx;line-height: 87rpx;border-bottom: 6rpx solid rgb(254, 222, 0);}
.content_03{width: 100%;display: flex;align-items: center;flex-wrap: wrap}
.fw_list{height: 60rpx;padding: 0 20rpx;font-size: 30rpx;line-height: 60rpx;margin: 30rpx 10rpx 0;border-radius:10rpx;}
.on{background:rgb(254, 222, 0);border:1rpx solid rgb(254, 222, 0);}
.off{border:1rpx solid #ccc;}
.btnBlack{width:690rpx;height: 96rpx;position: fixed;left: 50%;margin-left: -345rpx;bottom: 50rpx;}