              <view class='empty_text_01'>{{empty_text_01}}</view>
              <view class='empty_text_02'>{{empty_text_02}}</view>
            </view>
      </view>
      <!--可循环内容  --> 
      <view wx:for="{{list}}" wx:key="unique" class='content   border_bottom'>
