/* business/demand/demand03.wxss */
page{background: #f5f5f5}
.main{width: 690rpx;background: #fff;padding: 0 30rpx;}
.content_01{width: 100%;height: 87rpx;border-bottom:1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center;}
.content_title{font-size: 30rpx;color: #000;display: block;height: 87rpx;line-height: 87rpx;border-bottom: 6rpx solid rgb(254, 222, 0);}
.content_01_input{width: 690rpx;height: 87rpx;font-size: 30rpx;line-height: 87rpx;}
.main_02{width: 690rpx;background: #fff;padding: 0 30rpx 30rpx;margin-top: 30rpx;}
.main_02_textarea{width: 628rpx;height: 230rpx;border: 1rpx solid #dae2f8;background: #f4f4f4;border-radius: 10rpx;margin-top: 30rpx;padding: 30rpx;font-size: 30rpx;color: #8e9091;position: relative}
.num{font-size: 30rpx;color: #8e9091;position: absolute;right: 30rpx;bottom: 30rpx;}
.mb_list{width: 100%;height: 58rpx;margin-top: 20rpx;display: flex;}
.mb_style{width: 138rpx;height: 56rpx;line-height: 56rpx;font-size: 25rpx;text-align: center;border-radius: 5rpx;margin-right:30rpx; }
.on{background: #fede01;border: 1rpx solid #fede01;}
.off{border: 1rpx solid #bdbebe;}
.main_03{width: 690rpx;background: #fff;padding: 0 30rpx;margin-top: 30rpx;}
.main_03_input{width: 100%;height: 87rpx;border-bottom: 1rpx solid #e5e5e5;display: flex;justify-content: space-between;align-items: center}
.main_03_input_02{width: 100%;height: 87rpx;display: flex;justify-content: space-between;align-items: center}
.input_title{font-size: 34rpx;}
.input_main{display: flex;align-items: center;}
.input_main input{text-align: right;font-size: 30rpx;margin-right: 10rpx;}
.input_main text{font-size: 30rpx;}
.btnBlack{width:690rpx;height: 96rpx;position: fixed;left: 50%;margin-left: -345rpx;bottom: 50rpx;}
.picker{font-size: 30rpx;width: 500rpx;text-align: right}