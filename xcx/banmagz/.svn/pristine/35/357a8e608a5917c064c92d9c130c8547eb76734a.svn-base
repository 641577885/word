const bm = require('../utils/common.js')
const app = getApp()
Page({
  /**
   * 页面的初始数据
   */
  data: {
    isShowDialog: true,
    isreg:true
  },
  checkRole: function () {
    var that = this;
    if (app.globalData.auth == 0) {
      that.setData({
        isShowDialog: false
      })
    }
  }
})