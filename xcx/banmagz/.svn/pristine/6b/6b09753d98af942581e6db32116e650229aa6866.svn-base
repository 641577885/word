// pages/business/position/linkup.js
const bm = require('../../utils/common.js')
var app = getApp();
Page({

  /**
   * 页面的初始数据
   */
  data: {
    list:[]
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    var that = this
    var id = options.id
    bm.requsetData('/b/seller/chatLogDetail?id=' + id, 'get', '', function (data) {
      if(data.data.errcode == 0){
        that.setData({
          list:data.data.list,
        })
      }else{
        wx.showToast({
          title: data.data.errmsg,
          icon:'none'
        })
      }
      console.log(data)
    })
  },
})