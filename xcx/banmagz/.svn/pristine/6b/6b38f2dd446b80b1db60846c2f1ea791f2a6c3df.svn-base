/* pages/business/auth/busAuth.wxss */
page{background: #f4f4f4}
.top{width: 100%;height: 70rpx;background: #313131;text-align: center;font-size: 24rpx;color: #fede01;line-height: 70rpx;}
.content{width: 100%;background: #fff;padding-top: 60rpx;padding-bottom: 20rpx;}
.upimg{width: 470rpx;height: 280rpx;border: 1rpx solid #e5e5e5;margin:0 auto;position: relative;}
.upimgicon{display: block;width: 147rpx;height: 134rpx;position: absolute;top: 42rpx;left: 50%;margin-left: -73rpx}
.upimg text{width: 100%;text-align: center;font-size: 30rpx;line-height: 44rpx;position: absolute;top: 200rpx;left: 0;font-weight: bold}
.yl_img{width: 100%;height: 100%;position: absolute;top: 0;left: 0}
.content_text{display: block;width: 100%;text-align: center;font-size: 24rpx;color: #a9a9a9;line-height: 62rpx;margin-top: 10rpx;}
.info{width: 100%;background: #fff;margin-top: 30rpx;}
.info_main{width: 690rpx;height: 87rpx;padding-right: 30rpx;margin-left: 30rpx;display: flex;align-items: center;border-bottom:1rpx solid #e5e5e5; }
.info_main_02{width: 690rpx;height: 87rpx;padding-right: 30rpx;margin-left: 30rpx;display: flex;align-items: center;}
.info_title{width: 177rpx;font-size: 30rpx;color: #000;font-weight: bold}
.info_content{font-size: 24rpx;color: #a9a9a9;width: 400rpx;}
.btn{width: 580rpx;height: 76rpx;font-size: 30rpx;color: #000;line-height: 76rpx;text-align: center;border-radius: 38rpx;margin: 40rpx auto;background: linear-gradient(to right,#fede01,#fdc100);}
.input{width: 400rpx;}
.searchList{
  position: fixed;
  padding: 30rpx;
  width: 750rpx;
  box-sizing: border-box;
  background: #e5e5e5;
  z-index: 3;
}
.searchList view{ display: flex; align-items: center; justify-content: space-between; padding:8rpx 0;}
.adresTitle{ font-size: 28rpx; color: #666;}
.cityTxt{ font-size: 22rpx; color: #aaa;}