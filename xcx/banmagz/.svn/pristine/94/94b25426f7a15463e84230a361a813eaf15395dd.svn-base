/* help/help.wxss */
page{background: #fafafa}
.help{width: 100%;margin-top: 30rpx;background: #fff;padding-bottom:120rpx;padding-top: 20rpx;}
.main{width: 690rpx;margin-left: 30rpx;border-bottom: 1rpx solid #e5e5e5;margin-top: 40rpx;}
.title{width: 690rpx;font-size: 36rpx;color: #000;font-weight: bold;line-height: 60rpx;}
.content{width: 575rpx;font-size: 28rpx;color: #888888;line-height: 40rpx;margin-left: 42rpx;margin-top: 20rpx;margin-bottom: 20rpx;display: inline-block}
.kf_btn{width: 100%;height: 120rpx;position: fixed;left: 0;bottom: 0;background: #fff;display: flex;align-items: center}
.kf_btn button{width: 690rpx;background: #000;color: #fff}
