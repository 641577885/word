/* pages/business/position/linkup.wxss */
page{background: #fafafa}
.top{width: 690rpx;height: 100rpx;padding: 0 30rpx;font-size: 28rpx;color: #888888;line-height: 100rpx;}
.list{width: 100%;background: #fff;}
.list_main{width: 690rpx;margin-left: 30rpx;padding-right: 30rpx;border-bottom: 1rpx solid #eeeeee;padding-top: 34rpx;}
.list_main_01{width: 100%;height: 140rpx;display: flex;justify-content: space-between;}
.content_left{display: flex;}
.avatar{width: 138rpx;height: 138rpx;border: 1rpx solid #dfdfdf;border-radius: 4rpx;overflow: hidden;}
.avatar image{width: 138rpx;height: 138rpx;}
.info{margin-left: 20rpx;}
.name{font-size: 34rpx;line-height: 52rpx;}
.position{font-size: 26rpx;color: #a9a9a9;line-height: 44rpx;}
.price{font-size: 26rpx;line-height: 42rpx; margin-top: 5rpx;}
.yq{font-size: 24rpx;color: #bfbfbf;line-height: 38rpx;margin-top: 16rpx;text-align: center;}
.list_main_02{width: 690rpx;padding-right: 30rpx;margin-top: 27rpx;line-height: 37rpx;font-size: 28rpx;color: #666666}
.list_main_03{width: 100%;height: 50rpx;margin-top: 26rpx;display: flex;align-items: center;}
.icon_01{width: 30rpx;height: 30rpx;}
.icon_01 image{width: 30rpx;height: 30rpx;display: block;}
.icon_02{width: 28rpx;height: 30rpx;margin-left: 33rpx;}
.icon_02 image{width: 28rpx;height: 30rpx;display: block}
.num{font-size: 26rpx;color: #808191;line-height: 50rpx;margin-left: 14rpx;}
.scrolls{
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
}
.text_align{text-align: center;font-size: 28rpx;color: #a9a9a9;padding:10rpx; } 