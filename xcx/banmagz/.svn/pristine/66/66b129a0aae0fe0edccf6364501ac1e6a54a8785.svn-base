page{font-size: 28rpx;background: #f4f4f4;}
.jobCantainer{ position: absolute; top: 0; right: 0; left: 0}
.go_pc{width: 100%;height: 88rpx;background: #f6f0c3;}
.go_pc_content{width: 690rpx;height: 88rpx;margin: 0 auto;line-height: 88rpx;display: flex;justify-content: space-between;align-items: center;}
.go_pc_content image{width: 16rpx;height: 25rpx;}
.res_input{width: 690rpx;height: 88rpx;border-top: 1rpx solid #e5e5e5;background: #fff;display: flex;justify-content: space-between;align-items: center;padding-left: 30rpx;padding-right: 30rpx;font-size: 34rpx;}
.input_hd{ font-size: 30rpx;}
.input_bd{width: 500rpx;height: 88rpx;display: flex;align-items: center;}
.input_bg image{width: 16rpx;height: 25rpx;}
.picker{width: 100%;height: 88rpx;font-size: 26rpx;color: #bdbdbd;line-height: 88rpx;text-align: right;padding-right: 20rpx; overflow: hidden; white-space: nowrap; text-overflow: ellipsis; }
.res_input_02{margin-top: 29rpx;}
.prompt{padding-left:30rpx;height: 75rpx;font-size: 28rpx;color: #888888;line-height: 75rpx;}
.bottom_ts{display: block;width: 90%;font-size: 24rpx; line-height: 40rpx; color: #999999; text-align: center;margin: 28rpx auto;}
.open_btn{width: 690rpx;height: 96rpx;background: #000;color: #fff;font-size: 29rpx;line-height: 96rpx;margin-top: 20rpx;}
.del_btn{width: 690rpx;height: 96rpx;background: #ff4040;color: #fff;font-size: 29rpx;line-height: 96rpx;margin-top: 20rpx;}
.middle{width: 690rpx;height: 74rpx;margin: 0 auto;line-height: 74rpx;font-size: 28rpx;color: #888888;}
.btn_bg{width: 100%;background: #f4f4f4;position: fixed;left: 0;bottom: 0; padding: 20rpx 0;}
.fb_btn{width: 90%;height: 76rpx;border-radius: 10rpx; background: #fdc100 linear-gradient(to right, #fede01 , #fdc100);line-height: 76rpx;font-size: 30rpx;}
.necessary{position: relative;}
.necessary::before{position:absolute; content: "*"; left: -15rpx; top: 5rpx; color: #fe5a01; }

.input_text{width: 500rpx;height: 88rpx;font-size: 26rpx;color: #bdbdbd;line-height: 88rpx; text-align: right; padding-right: 20rpx;}
.bottom30{ margin-bottom: 30rpx;}
.CoverView{ position: absolute; top: 0;left: 0;right: 0; bottom: 0; background: rgba(0,0,0,0.6);}
.CoverViewItem{ position: absolute; bottom: 0; width: 100%; left: 0; right: 0; background: #fff}
.cancelView{ height: 80rpx; display: flex; align-items: center; justify-content: space-between; padding: 0 50rpx; border-bottom: 1rpx solid #eee}
picker-view-column view{ text-align: center; height: 40px !important}
.cancel,.click{ font-size: 36rpx;}
.click{ color: #1aad19;}
.priceTxt{color: #8e9091; font-size: 26rpx;}