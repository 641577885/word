page{height: 100%;background: #f4f4f4}
.fillContanier{width: 100%}
.fillTop{width: 100%;background: #ffffff}
.fillTopCon{ position: relative; width: 666rpx;margin: 0 auto;padding: 50rpx 0}
.cancelBtn{ position: absolute; right:0; top: 50rpx;font-size: 30rpx; color: #0060ff}
.fillInfo{display: flex;justify-content: space-between}
.fillText{font-size: 36rpx;font-weight: bold;padding-bottom: 30rpx}
.warn{font-size: 24rpx;color: #666666}  

.Cantainer{ position: absolute; top: 350rpx; bottom: 180rpx; left: 0; right: 0;  background: #fff}
.tagTip{ text-align: center;height: 60rpx; line-height: 60rpx; font-size: 24rpx; color: #666; margin-top: 20rpx; background: #fff;}
.tagList{ padding: 10rpx 30rpx;}
.tagList view{ display: inline-block; height: 48rpx; font-size: 26rpx; line-height:48rpx; color: #333; text-align: center; border: 1rpx solid #e5e5e5; border-radius: 5rpx; vertical-align: top; padding: 0 20rpx; margin: 0 20rpx 20rpx 0; box-sizing: border-box }
.tagList view.action{ background: #fede01; border-color: #fede01;}

/* bottom */
.Bottom{position: fixed;bottom: 20rpx;left: 42rpx;text-align: center;}
.notsure{width: 666rpx;background: none; line-height: 40rpx; font-size: 28rpx;margin-bottom: 10rpx;}
.next{width: 666rpx;height: 96rpx;background: #000000;line-height: 96rpx;color: #ffffff;font-size: 29rpx}
button::after{border: none}
