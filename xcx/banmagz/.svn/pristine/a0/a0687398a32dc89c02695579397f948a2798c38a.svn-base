/* message/bmDialog.wxss */
page{background: #f5f5f5}
.content{width: 100%;height: 100%;}
.list{width: 100%}
.msg_content{width: 690rpx;margin: 30rpx auto 0;background: #fff;border-radius: 7rpx;padding:22rpx 0}
.title{font-size: 36rpx;line-height: 52rpx;width: 646rpx;margin: 0 auto; word-break:break-all}
.img{width: 646rpx;height: 350rpx;background: #f5f5f5;border-radius: 7rpx;margin: 10rpx auto 0;display: flex;align-items: center}
.img image{width: 100%}
.main{font-size: 30rpx;line-height: 52rpx;width: 646rpx;margin: 0 auto; word-break:break-all;color: #666666}
.msgempty{width: 100%;height: 50rpx;font-size: 50rpx;line-height: 50rpx;color: #ccc;font-weight: bold;text-align: center;position: fixed;left: 0;top: 40%}