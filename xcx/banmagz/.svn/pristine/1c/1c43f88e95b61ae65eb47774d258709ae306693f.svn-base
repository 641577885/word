<view class='cantainer'>
  <navigator url='/pages/business/demand' class='cancelBtn' open-type="reLaunch">取消</navigator>
  <text class='publishtypeTitle'>选择发布类型</text>
  <text class='publishtypeDescript'>请选择您要发布的对应类型</text>
  <view class='publishtypeItem worker' bindtap='recruit'>
    <image src='/images/member/workIcon.png'></image>
    <view class='publishtypeDetail'>
      <text class='publishtypeIdent'>招人入职</text>
      <text class='publishtypeInfo'>轻松寻找行业优秀人才</text>
    </view>
  </view>
  <view class='publishtypeItem boss' bindtap='findJob'>
    <image src='/images/member/bossIcon.png'></image>
    <view class='publishtypeDetail'>
      <text class='publishtypeIdent'>找人做项目</text>
      <text class='publishtypeInfo'>完成人才对接</text>
      <text class='publishtypeInfo'>快速启动项目</text>
    </view>
  </view>
</view>
