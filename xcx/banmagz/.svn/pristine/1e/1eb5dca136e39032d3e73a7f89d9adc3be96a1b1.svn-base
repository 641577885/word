var app = getApp();
var bm = require('../../utils/common.js')
Page({

  /**
   * 页面的初始数据
   */
  data: {

    url: ''

  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    wx.showToast({
      mask: true,
      title: '正在刷新',
      icon: 'loading',
      duration: 500
    })
    var tk = app.globalData.token
    var host = app.globalData.host
    var url = host +'/html/applyticket/apply.html?tk=' + tk

    this.setData({ url: url })
  }
})