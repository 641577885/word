// pages/report/report.js
var app=getApp();
Page({
  /**
   * 页面的初始数据
   */
  data: {
    url:''
  },
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    var token = app.globalData.token
    var host = app.globalData.host
    var url = host+'/html/report/report.html'
    this.setData({ url: url })
  }
})