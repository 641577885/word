}

/* 评价 */

.posbottoms {
  position: fixed;
  bottom: 96rpx;
  left: 13rpx;
}

.evaluate {
  width: 144rpx;
  height: 46rpx;
  color: #0060ff;
  line-height: 46rpx;
  font-size: 22rpx;
  text-align: center;
  border: 1rpx solid #0060ff;
  float: right;
  margin-right: 30rpx;
  margin-top: 10rpx;
  border-radius: 5rpx;
}

.hireonce {
  width: 100%;
  height: 76rpx;
  background: #fedd01;
  color: #000;
  line-height: 70rpx;
  font-size: 29rpx;
  margin-top: 10rpx;
}

.showblock {
  display: flex;
}

.shownone {
  display: none;
}

.manageCantainer {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  padding: 20rpx;
  background: #fff;
  box-sizing: border-box;
}

.manageCantainer button {
  font-size: 34rpx;
}

button::after {
  border: none;
}

button {
  background: #fff;
}

.addbtn {
  position: absolute;
  right: 56rpx;
  bottom: 55rpx;
}

.addbtnimg {
  width: 110rpx;
  height: 110rpx;
  box-shadow: 4rpx 15rpx 100rpx #fed401;
  border-radius: 50%;
}

.sharebutton::after {
  border: none;
}

action-sheet {
  background: #fff;
}

.action_item {
  height: 100rpx;
  width: 690rpx;
  margin: 0 auto;
  padding: 0;
  border-bottom: 1rpx solid #ccc;
}

.action_item:last-child {
  border-bottom: 0 none;
}

.sharebutton {
  height: 100%;
  line-height: 100rpx;
  text-align: center;
  color: #333;
}
.posconfirm {
  position: fixed;
  background: #fff;
  padding: 10rpx;
  bottom: 0;
  left: 0;
  right: 0
}

.workStatusCantainer,.workContent{ border-top: 30rpx solid #fafafa;}
.workStatusTitle{ padding-left: 30rpx;border-top: 5rpx solid #fe5a01; }
.workStatusTxt{ border-bottom: 1rpx solid #eeeeee;padding-top: 12rpx; height: 70rpx; line-height: 70rpx; font-size: 26rpx; color: #333333; }
.workStatusTxt text{ color: #fe5a01;}
.workStatusNote{ padding: 30rpx;}
.workbody{ color: #333; font-size: 26rpx; line-height: 36rpx;}
.workImgCantainer{ display: flex; justify-content: flex-start; }
.workImgItem{ width: 138rpx; height: 138rpx; border-radius:10rpx; overflow: hidden; margin: 12rpx 12rpx 0 0; }
.workImgItem image{ width: 100%; height: 100%;}
.workContent{ padding-left: 30rpx;}

.explainCantainer{ background: #fffaec; border: 1rpx solid #eee; padding: 20rpx; width: 688rpx; box-sizing: border-box; margin: 0 auto; border-radius: 5rpx; margin-bottom: 30rpx;}
.explaintxt{ font-size: 26rpx; color: #333; margin-bottom: 10rpx;}  
/* 投诉 */
.complain{width: 690rpx;margin: 0 auto;font-size: 26rpx;color: #fe5a01;}
.gohelp{display: inline;color: blue;text-decoration: underline}