/* pages/member/publishtype.wxss */
.cantainer{ position: relative; overflow: hidden}
.cancelBtn{ position: absolute; right: 20rpx; top: 50rpx; font-size: 30rpx; color: #0060ff}
.publishtypeTitle{ display: block; font-size: 56rpx; margin-top: 92rpx; text-align: center}
.publishtypeDescript{ display: block; font-size: 28rpx; color: #8e9091; margin-top: 25rpx; text-align: center}
.publishtypeItem{ position: relative; width: 558rpx; height: 240rpx; background: #fff; border-radius: 14rpx; box-shadow: 0rpx 10rpx 90rpx 0rpx rgba(13,16,42,0.26); margin: 120rpx auto 60rpx;}
.worker image{ position: absolute; bottom:0; left: -20rpx; width: 232rpx; height: 312rpx; }
.boss image{ position: absolute; bottom:0; left: -36rpx; width: 263rpx; height: 336rpx; }
.publishtypeDetail{ position: absolute; left: 260rpx; top: 70rpx;}
.publishtypeDetail text{ display: block;}
.publishtypeIdent{ font-size: 36rpx; font-weight: bold;}
.publishtypeInfo{ font-size: 26rpx; color: #666; line-height: 40rpx;}