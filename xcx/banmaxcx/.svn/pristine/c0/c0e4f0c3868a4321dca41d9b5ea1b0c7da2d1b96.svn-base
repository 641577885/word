  closeresume:function(e){
    var close = e.detail.value
    if(close){
      var is_show=1
    }else{
      var is_show=0
    }
    bm.requsetData('/w/resume/show', 'post', { is_show: is_show }, function (data) {
      if(data.data.errcode !=0){
        wx.showToast({
          title: data.data.msg,
          icon:'none'
        })
      }
    })
  }
})