.list_main{width: 690rpx;margin-left:30rpx;padding-right:30rpx;border-bottom: 1rpx solid #eeeeee;padding-top: 30rpx;padding-bottom: 30rpx;}
.tabbar{width: 100%;height: 87rpx;border-top: 1rpx solid #cccccc;background: #f7f7fa;position: fixed;left: 0;bottom: 0;display: flex;justify-content: space-around;}
.tabbar_content{height: 87rpx;font-size: 0}
.tabbar_icon{width: 50rpx;height: 50rpx;margin-top: 10rpx;}
.tabbar_icon02{width: 46rpx;height: 46rpx;}
.tabbar_content view{display: block;font-size: 20rpx;color: #7f8389;line-height: 24rpx;margin-top: 0rpx;text-align: center}
