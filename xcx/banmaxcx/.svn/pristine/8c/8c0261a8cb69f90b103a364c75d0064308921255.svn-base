.list_top{display: flex;align-items: center;justify-content: space-between;height: 56rpx;}
.position_name{font-size: 34rpx;font-weight: bold;}
.price{display: flex;align-items: center;height: 56rpx;}
.price_title{font-size: 26rpx;color: #999999;}
.money{font-size: 36rpx;color: #fe5a01;margin-left: 15rpx;font-weight: bold}
.list_content{line-height: 46rpx;font-size: 28rpx;color: #666666;margin: 20rpx 0;word-break:break-all;}
.list_bottom{display: flex;align-items: center;height: 58rpx;}
.avatar{width: 58rpx;height: 58rpx;border-radius: 50%;overflow: hidden;background: #dfdfdf}
.avatar image{width: 100%;height: 100%}
.list_info{margin-left: 22rpx;font-size: 26rpx}
