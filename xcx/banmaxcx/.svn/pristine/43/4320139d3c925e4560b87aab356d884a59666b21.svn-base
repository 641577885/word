/* wallet/invited.wxss */
page{height: 100%;background: #f4f4f4}
.missCantainer{ position: absolute; top: 0; left: 0; bottom: 150rpx; right:0; overflow-y: auto}
.inviteTip{width: 100%;height: 88rpx;background: #f6f0c3;line-height: 88rpx;display: flex;font-size: 28rpx;position:fixed;top:0;left:0;}
.report{font-weight: bold;padding: 0 0rpx 0 35rpx;width: 150rpx}
 /* .inviteBanner{}  */
.banner{margin-top:88rpx;}
.inviteBanner image{width: 100%;height: 411rpx}
.inMainTop{width: 100%;background: #ffffff}
.inMainMain{width: 650rpx;margin: 0 auto}
.mainBoth{display: flex;justify-content: space-between;padding: 48rpx 0 30rpx 0}
.record{font-size: 36rpx;font-weight: bold}
.situation{font-size: 30rpx;color: #333333}
.inNav{width: 100%;height: 17rpx;border-radius: 6rpx;background: #f4f4f4}
 /* .inNav slider{height: 17rpx;}  */
 .numBoth{display: flex}
 .numBoth text{width: 130rpx;font-size: 24rpx;padding: 14rpx 0 40rpx 22rpx}
 .inMianCenter{margin-top: 30rpx;width: 100%}
 .inCenterBoth{width: 650rpx;margin: 0 auto;background: #fff}
 /* //mainCenter */
.mocContainer{width:650rpx;justify-content:space-between;box-sizing:border-box;
/* margin-top:30rpx; */margin:30rpx auto;}
/* .tab-box{height:100%;} */
/* .tab{height: 100%}  */
.tab-nav{height: 80rpx;line-height: 80rpx;border-bottom: 2rpx solid #f4f4f4;position:absolute;z-index:999;width:650rpx;display:flex;justify-content:space-between;background:#fff;}
.tab-nav view{ height: 80rpx;line-height: 80rpx;font-size: 36rpx;font-weight: bold;margin-left:40rpx;}
.tab-nav view.on{border-bottom: 7rpx solid #f4d721;color: #000000;}
.asdasd{height: 68%;overflow-x: hidden;box-sizing:border-box;}
.box{background:#ffffff;height: 100%;}
swiper-item{top: 80rpx}
.discount{text-align: center;font-size: 26rpx;padding: 100rpx 0 8rpx 0}
.inList{width: 570rpx;margin: 0 auto;display: flex;margin-bottom: 25rpx}
.listLeft{overflow: hidden;padding-right: 19rpx}
.listLeft image{width: 80rpx;height: 80rpx;border-radius: 50%}
.listRight{font-size: 30rpx}
.listname{color: #888888}
.inBottom{width: 100%;height: 148rpx;position: fixed;bottom: 0;left: 0;border: 1rpx solid #cccccc;background: #fff}
.inBottomMain{width: 570rpx;margin: 0 auto;display: flex;padding-top: 43rpx}
.inBottomMain button{width: 273rpx;height: 74rpx;line-height: 74rpx;text-align: center;background: none;font-size: 29rpx}
.active{background:linear-gradient(to right,#fede01,#fdc100) !important;}
.bannerText{width: 516rpx;}
.srcoll_view{position: absolute;top:0rpx;height: 100rpx;font-size: 30rpx;white-space: nowrap;line-height: 100rpx;color: #000;animation:myfirst 30s linear infinite;display: flex}
