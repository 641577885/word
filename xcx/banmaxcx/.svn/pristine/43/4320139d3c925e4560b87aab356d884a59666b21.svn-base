.scroll_view_border{position: relative;width: 100%;height: 100rpx;overflow: hidden;display: flex;white-space: nowrap}
.empty_icon{width: 100%;height: 150rpx;position: initial;left: 0;top: 0;margin-left: 0}
.empty_icon image{width: 150rpx;height: 150rpx;margin: 0 auto;display: block;}
.empty_text_01{font-size: 24rpx;text-align: center;line-height: 50px}
.explainCantainer{background: #fff;padding-top:50rpx}
.actived{border-bottom: 5rpx solid #fede01;display:initial;}
/* 跑马灯 */
.gonggao {
  width: 80%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;
}
.scrolltxt{
  padding:0 20rpx;
}
.marquee_box {
  position:relative;
  color:#333;
  height:100rpx;
  display:block;
  overflow:hidden;
} 
.marquee_text {
  white-space: nowrap;
  position:absolute;
  top:0;
  font-size:14px;
  /* height: 100rpx;
  line-height:100rpx; */
}


