.msgCantainer{position: absolute; top: 0; left: 0; bottom: 0; right: 0; background: #f5f5f5; overflow: hidden;}
.opcantainer{ display: flex; align-items: center; justify-content: space-around; width: 100%; height: 88rpx; border-bottom: 1rpx solid #dadada; background: #fff;}
.opcantainer view{ flex: 1; height: 100%; line-height: 88rpx; font-size: 30rpx; color: #353535; text-align: center; border-right: 1rpx solid #dadada; }
.opcantainer view:last-child{ border: 0 none}
.msgProItem{ display: flex; justify-content: space-between; width: 690rpx; padding: 35rpx 30rpx; background: #fff; border-radius: 10rpx; margin: 20rpx auto; box-sizing: border-box; flex-direction: column;}
.msgProTop{ display: flex;  justify-content: space-between}
.msgProDetail{ width: 450rpx; height: auto; font-size: 34rpx; color: #000;}
.msgProPrice,.msgJobPrice{ font-size: 36rpx; color: #fe5a01; text-align: right}
.msgProStatusItem{ width: 100%; display: flex; justify-content: space-between; margin-top: 30rpx;}
.msgProStatus{ font-size: 24rpx;}
.msgProStatus.normal{ color:#bfbfbf;}
.msgProStatus.abnormal{ color: #fe5a01;}
.msgProTime{ font-size: 24rpx; color: #bfbfbf}
.msgJobItem{ align-items: center;}
.msgJobName{ font-size: 28rpx;}
.msgJobTip{ font-size: 24rpx; color: #bfbfbf;}
.msgJobPriceItem{ text-align: right;}
.DialogCantainer{height: 600rpx;}
.dialogItem{ padding: 0 30rpx; width: 100%; overflow: hidden; margin-bottom: 30rpx;  box-sizing: border-box; }
.dialogThumb{ float: left; width: 100rpx; height: 100rpx; border-radius: 50%; vertical-align: top; margin: 0 20rpx 0 0; overflow: hidden;}
.dialogThumb image{ width: 100%; height: 100%}
.dialogCover { position: relative; float: left; max-width: 570rpx; height: auto; padding: 26rpx 35rpx; background: #fff; border-radius: 5rpx; font-size: 30rpx; color: #333; line-height: 37rpx; box-sizing: border-box; vertical-align: top; word-break: break-all;}
.selfItem .dialogThumb{ float: right; margin:0 0 0 20rpx;}
.selfItem .dialogCover{ float: right; border-top-right-radius: 0;}
.fromItem .dialogCover{ border-top-left-radius: 0;}
.fromItem .dialogCover::before{ position: absolute; left: -5rpx; top: 0; display: block; content: ""; width: 6rpx; height: 7rpx; background: url(http://img.jiangcdn.com/bmjx/images/msg/dialogLeft.png) no-repeat; background-size: 100%;}
.selfItem .dialogCover::before{position: absolute; right: -5rpx; top: 0; display: block; content: ""; width: 6rpx; height: 7rpx; background: url(http://img.jiangcdn.com/bmjx/images/msg/dialogRight.png) no-repeat; background-size: 100%;}
.dialogImage{ float: left; height:auto; padding: 5rpx;  background:#fff; border-radius: 5rpx;}
.selfItem .dialogImage{ float: right;}
.dialogImage image{ max-width: 300rpx; }

.dialogFile{ display: inline-block; width: 95rpx; height: 92rpx; vertical-align: top;}
.dialogFile image{ width: 100%;}
.dialogFileInfo{ display: inline-block; width: 230rpx; height: auto; margin-left: 18rpx;}
.dialogFileName{ display: block; font-size: 30rpx; color: #333; white-space: nowrap; text-overflow: ellipsis;}
.dialogFileDetail{ display: flex; width: 100%; justify-content: space-between; margin-top: 20rpx;}
.dialogFileSize{ font-size: 24rpx; color: #666;}
.dialogFileTerm{ font-size: 24rpx; color: #999; }

.dialogWechat{ font-size: 30rpx; color: #333;}
.dialogWechatBtn{ display: flex; width: 314rpx;justify-content: space-between; margin-top: 30rpx;}
.dialogWechatBtn button{ width: 152rpx; height: 66rpx; font-size: 24rpx; color: #000; line-height: 66rpx; box-sizing: border-box;}
.refuse{ background: none;}
.agree{ background: #fede01;}

.dialogInfoCopy{ display: flex; justify-content: space-between;}
.infoDetail{ position: relative; font-size: 24rpx; color: #000000; padding-left: 35rpx;}
.copyBtn{ font-size: 24rpx; color: #0060ff; margin-left: 20rpx;}
.infoDetail::before{ position: absolute; left: 0; content: ""; display: block; }
.cell::before{ top: 5rpx; width: 26rpx; height: 27rpx; background: url(http://img.jiangcdn.com/bmjx/images/msg/cell_Icon.png) no-repeat; background-size: 100%;}
.wechat::before{ top: 5rpx; width: 33rpx; height: 26rpx; background: url(http://img.jiangcdn.com/bmjx/images/msg/wechat_Icon.png) no-repeat; background-size: 100%;}

.dialogSendCantainer{ position: absolute; bottom: 0; left: 0; right: 0; background: #fff;}
.dialogSendTop{ display: flex; padding: 17rpx 16rpx; justify-content: space-between; }
.dialogCommon{  width: 130rpx; height: 66rpx; text-align: center; line-height: 66rpx; background: #000000; font-size: 30rpx; color: #ffffff; border-radius: 5rpx; }
.sendPlaceHolder{ height: 66rpx; line-height: 66rpx; font-size: 30rpx;}
.sendContent{ display: block; width: 500rpx; font-size: 30rpx; height: 66rpx; color: #000;}
.MoreSend{  width: 66rpx; height: 66rpx; background: url(http://img.jiangcdn.com/bmjx/images/msg/addMore_Icon.png) no-repeat; background-size: 100%;}

.moreCantainer{ display: flex; justify-content: space-around; align-items: center; height: 0; overflow: hidden; transition: height 0.1s ease;}
.showH{ height: 152rpx;}
.moreItem{ width: 120rpx; text-align: center; padding: 20rpx 0;}
.moreItem image{ width: 86rpx; height: 70rpx;}
.moreItem text{ display: block; text-align: center; font-size: 24rpx; color: #666;}