.cancelView{ width: 20%;}
.firstView{ width: 35%; background: #f5f5f5; padding-bottom: 112rpx;}
.secondView{ width: 45%; background: #fff; padding-bottom: 112rpx;}
.firstView view{ display: flex; align-items: center;height: 91rpx; font-size: 26rpx; color: #000; border-right: 1rpx solid #e5e5e5; box-sizing: border-box;padding: 0 18rpx}
.firstView view.action{ background: #fff; border-bottom: 1rpx solid #e5e5e5; border-right: 1rpx solid #fff;}
.silderButton{ display: flex; align-items: center; justify-content: space-between; position: absolute; right: 0; bottom: 0; background: #fff; width: 80%; padding: 10rpx; box-sizing: border-box;}
.reset{ width: 270rpx; background: #fdc100 linear-gradient(to right,#fedd01 , #fdc100);border-radius: 10rpx 0 0 10rpx}
.submit{ width: 360rpx; color: #fff; background: #ff5d3e;border-radius: 0 10rpx 10rpx 0}
.reset::after,.submit::after{ border: 0 none; margin: 0}
.viewicon{ position: relative; width: 44rpx;height: 44rpx;border: 1rpx solid #c9c9c9;border-radius: 50%; box-sizing: border-box}
.success_icon{ position: absolute; top: 0rpx; left: 0rpx;width: 100%;height: 100%}

.secviewItem{ display: flex; align-items: center; padding-left: 24rpx; height: 91rpx;}
.secviewTxt{ height: 100%; width: 230rpx; line-height: 90rpx; font-size: 30rpx; color: #000; border-bottom: 1rpx solid #e5e5e5; margin-left: 26rpx;}.position_fixed{position:fixed; width: 100%}



