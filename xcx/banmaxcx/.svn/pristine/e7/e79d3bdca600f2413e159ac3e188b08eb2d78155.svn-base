.manageCantainer {
  position: fixed;
  width: 100%;
  bottom: 0;
  left: 0;
  padding: 20rpx;
  background: #fff;
  box-sizing: border-box;
}

.manageCantainer button {
  font-size: 34rpx;
}

button::after {
  border: none;
}

button {
  background: #fff;
}

.addbtn {
  position: fixed;
  right: 56rpx;
  bottom: 55rpx;
}

.addbtnimg {
  width: 110rpx;
  height: 110rpx;
