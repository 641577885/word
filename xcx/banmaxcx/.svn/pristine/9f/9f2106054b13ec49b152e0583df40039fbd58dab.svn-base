/* record/uprecord.wxss */
/* .clearf:after{ content:"."; clear:both; display:block; height:0; visibility:hidden;} */
page{height: 100%;background: #f4f4f4}
body{
	background: #f4f4f4;
	font-size: 34rpx;
}
.deliver{
	width: 100%;
}
.deliver_list{
	display: block;
	background: #fff;
	margin-bottom:30rpx;
	padding: 20rpx 30rpx;
}

.deliverAll{position: absolute;width: 100%;top: 0;bottom: 50rpx}
.record{text-align: center;font-size: 36rpx;}
.deliver_list_info{
	width: 100%;
	height: 70rpx;
  display: flex;
  justify-content: space-between
}
.list_job{display: flex}
.online{
	display: inline-block;
	/* width: 188rpx; */
	height: 42rpx;
	 background: linear-gradient(to right,#fede01,#fdc100); 
  /* background:#fdc100 linear-gradient(to right,#fedd01 , #fdc100); */

	line-height: 42rpx;
	font-size: 26rpx;
	text-align: center;
	border-radius: 3rpx;
	margin-left: 20rpx;
  padding: 0 20rpx;
}
.list_price{
	line-height: 42rpx;
	color: #fe5a01;
	font-size: 36rpx;
}
.deliver_list_company{
	font-size: 28rpx;
	color: #a9a9a9;
	line-height: 44rpx;
	margin-top: 10rpx;
}
.deliver_list_x{
	width: 100%;
	height: 2rpx;
	background:#eeeeee;
	margin-top: 21rpx;
}
.deliver_list_person{
	width: 100%;
	height: 58rpx;
	margin-top: 20rpx;
  display: flex;justify-content: space-between
}
.list_avatar{
	width: 58rpx;
	height: 58rpx;
	border-radius: 50%;
	background: #dfdfdf;
	overflow: hidden;
}
.img{width: 58rpx;height: 58rpx;border-radius: 50%}
.list_name{
	font-size: 26rpx;
	line-height: 58rpx;
	margin-left: 22rpx;
}
.list_time{
	font-size: 24rpx;
	color: #bfbfbf;
	line-height: 58rpx;
}
.deliver_both{display: flex;}

.explainCantainer{ position: absolute; top: 88rpx; bottom: 0; left: 0; right: 0}
.display_none{display: none}
.tab-box{position: absolute;top: 0;bottom: 0}
.box{position: absolute;top: 0;bottom:0rpx}
.plat_right{font-size:26rpx;
background:linear-gradient(to right,#ff6f40 , #ff4140);
color:#FFFFFF;
border-radius:4rpx;
/* padding:0 18rpx; */
text-align:center;
height:42rpx;
line-height:42rpx;
margin:8rpx 0 0 20rpx;
}