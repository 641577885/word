/* help/help.wxss */
page{background: #fafafa}
.help{width: 100%;margin-top: 30rpx;}
.help textarea{width: 630rpx;height: 360rpx;background: #fff;border: 1rpx solid #f5f5f5;border-radius: 10rpx;margin: 0 auto;padding: 30rpx;font-size: 30rpx;color: #999}
.title{font-size: 30rpx;padding: 0 40rpx;color: #999;line-height: 40rpx;height: 60rpx}
.btn{width: 600rpx;margin: 30rpx auto;background: #000;color: #fff}
.kf_btn{width: 100%;height: 120rpx;position: fixed;left: 0;bottom: 0;background: #fff;display: flex;align-items: center}
.kf_btn button{width: 690rpx;background: #000;color: #fff}
