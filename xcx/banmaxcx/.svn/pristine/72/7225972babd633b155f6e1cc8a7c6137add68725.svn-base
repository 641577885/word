/* pages/index/index.wxss */
.topbar{ display: flex; align-items: center; justify-content: space-between; width: 100%; height: 78rpx; background: #fede01;}
.topbarLeft{ display: flex; align-items: center; justify-content: flex-start; width: 655rpx; margin-left: 20rpx;}
.myIcon{ display: flex; align-items: center; justify-content: space-around; width: 78rpx; height: 78rpx;}
.myIcon image{ width: 26rpx; height: 32rpx;}
.topbarTxt{ width: 145rpx; font-size: 30rpx; height: 78rpx; line-height: 78rpx; color: #000;}
.topbarTxt.action{ font-weight: bold;}
.topbarRight{ display: flex; align-items: center; justify-content: space-around; width: 95rpx;}
.msgBtn{ display: flex; align-items: center; justify-content: space-around; width: 78rpx; height: 78rpx;}
.msgBtn image{ width: 30rpx; height: 30rpx;}

.mapCantainer{ position: absolute; top: 78rpx; bottom: 120rpx; width:100%; height: auto;}
.demandCantainer{ display: flex; align-items: center; justify-content: space-between; position: absolute; bottom: 0; left: 0; right: 0; padding: 20rpx; background: #fff; z-index: 1}
.demandInfo{ padding: 0 30rpx;}
.tipCantainer{ display: flex; align-items: center; justify-content: flex-start; height: 80rpx; background: #f5f5f5; padding: 0 30rpx;}
.tipCantainer image{ width: 26rpx; height: 26rpx;}
.tipCantainer text{ font-size: 24rpx; color: #666666; margin-left: 15rpx;}
.inpCantainer{ display: flex; align-items: center; justify-content: space-between; height: 98rpx; border-bottom: 1rpx solid #e5e5e5}
.iconCantianer{ display: flex; align-items: center; justify-content: space-around; width: 50rpx; height: 50rpx;}
.inpCantainer .position{ width: 36rpx; height: 32rpx;}
.inpCantainer .money{ width: 32rpx; height: 35rpx;}

.findBtn{ width: 85%; height: 80rpx; text-align: center; border-radius: 10rpx; line-height: 80rpx; background: #000; color: #fff; font-size: 32rpx; }
.back{ display: flex; height: 80rpx; width: 80rpx;  border-radius: 50%; background: #000; justify-content: center; align-items: center}
.demandCantainer image{ width: 50rpx; height: 50rpx;}