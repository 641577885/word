 page{height: 100%}
 .mocContanier{width: 100%;height: 100%;background: #f4f4f4;overflow: hidden}
.mocMain{width: 690rpx;margin: 30rpx auto;background: #fff;height: auto;} 
.mocNum{font-size: 36rpx;color: #fe5a01}
.mocName{font-size: 30rpx;}
.mocBoth{display: flex;justify-content:space-between;}
.inputnum{font-size: 52rpx;padding: 30rpx 0;color: #000000;border-bottom: 1rpx solid #e5e5e5;margin-bottom: 30rpx}
.detail{font-size: 24rpx;color: #a9a9a9;padding-bottom: 18rpx}
.mocchange{font-size: 24rpx;color: #a9a9a9;}
.mocMainCon{padding: 41rpx 36rpx;}
.order{font-size: 24rpx;color: #a9a9a9;}
.close{font-size: 24rpx;color: #000000;line-height: 80rpx;border-bottom: 1rpx solid #e5e5e5}
.close text{color: red}
.section{display: flex;padding-top: 50rpx;align-items: center;}
.section_02{display: flex;margin-top: 50rpx;justify-content: space-between;align-items: center;height: 60rpx;}
.moc_icon{width: 40rpx;height: 40rpx;border: 1rpx solid #a9a9a9;border-radius: 4rpx;padding: 3rpx}
.moc_icon_02{width: 40rpx;height: 40rpx;border: 1rpx solid #c9c9c9;border-radius: 4rpx;background: #09bb07;padding: 3rpx;transform: scale(0.8)}
.moc_left{display: flex}
.moc_money{width: 100%;height: 100rpx;display: flex;margin-top: 50rpx;justify-content: space-between;align-items: center;border-top:1rpx solid #e5e5e5;}
.moc_money_left{font-size: 30rpx;}
.moc_money_right{font-size: 36rpx;color: #fe5a01}
.balan,.money{font-size: 30rpx;color: #000000;line-height: 60rpx}
.balan{padding: 0 17rpx 0 20rpx;}
.money text{color: #fe5a01}
.group{width: 46rpx;height: 46rpx;border: 1rpx solid #c9c9c9;border-radius: 50%;position: relative;transform: scale(0.8)}
.group icon{position: absolute;left: -1rpx;top: -1rpx;}
.show{display: block;}
.hide{display: none}
/* 底部 */
.rnameBottom{width: 580rpx;margin: 20rpx auto 0;padding-bottom: 70rpx;}
.nextbtn{height: 76rpx;background: linear-gradient(to right,#fede01,#fdc100);border-radius: 36rpx;line-height: 76rpx;font-size: 30rpx;}
.bottom_info{width: 100%;height: 100rpx;line-height: 100rpx;font-size: 30rpx;color: #888888;position: fixed;left: 0;bottom: 0;text-align: center}
.share_btn::after{
  border: 0
}
.share_btn{background: #fff;font-size: 30rpx;height: 40rpx;width: 500rpx;margin: 30rpx auto 0;line-height: 40rpx;}
.dialog_screen{ position: fixed; top: 0; left: 0; background: #000; opacity: 0.2; overflow: hidden; width: 100%;height: 100%;}
.dialog_box{ position: fixed; top: 30%; left: 10%; background: #fff; width: 80%; padding: 50rpx 20rpx; border-radius: 10rpx; box-sizing: border-box; }
.fillcodecantainer{ position: relative; display: flex; width: 100%; justify-content: space-around; box-sizing: border-box; margin-top: 20rpx; }
.codeInp{width: 0; height: 0;text-indent: 999em; opacity: 0}
.telInfo{ display: flex; align-items: center; justify-content: space-between; padding: 0 20rpx;}
.lxtelNumber{ display: block; font-size: 34rpx; color: #999;}
.codeResend{  text-align: center; margin: 0; font-size: 32rpx;}
.codeItem{ width: 15%; height: 86rpx; line-height: 86rpx; text-align: center; border-bottom:1rpx solid #999; box-sizing: border-box; margin: 0 10rpx;}
button::after{border: none}