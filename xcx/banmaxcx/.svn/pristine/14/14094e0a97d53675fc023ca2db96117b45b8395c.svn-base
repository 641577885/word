<!--pages/login/dialog.wxml-->
<view class='loginCover' hidden='{{dialogHidden}}'>
  <view class='loginDialog'>
    <text class='loginTitle'>登录或注册班马</text>
    <button class='green' open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber">微信帐号快速登录</button>
    <navigator url="/member/bindPhone" hover-class='none'><button>手机号登录</button></navigator>
  </view>
</view>

